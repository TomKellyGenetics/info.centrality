citHeader("To cite info.centrality in publications use:")

citEntry(entry = "Manual",
  title        = "{info.centrality}: an R implementation of information centrality using igraph",
  author       = personList(as.person("S. Thomas Kelly")) 
  year         = "2018",
  note         = "R package version 0.1.0"
  url          = "https://github.com/TomKellyGenetics/info.centrality",

  textVersion  =
  paste("S. Thomas Kelly (2018).",
        "{info.centrality}: an R implementation of information centrality using igraph",
        "R package version 0.1.0",
        "URL https://github.com/TomKellyGenetics/info.centrality/")
)

